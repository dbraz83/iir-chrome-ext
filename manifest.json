{
  "name": "IIR Chrome Extension",
  "version": "0.2",
  "manifest_version": 2,
  "description": "Not just mucking about now, this thing is starting to resembling a chrome extension",
  "background": {
    "scripts": [
      "jquery-1.11.1.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "IIR Chrome Extension",
    "default_popup": "popup.html"
},
  "permissions": [
    "storage",
    "bookmarks",
    "tabs",
    "cookies",
    "unlimitedStorage",
    "contextMenus",
    "*://*.google.com/*",
    "*://*.google.co.uk/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://www.google.co.uk/search*",
        "*://www.google.com/search*"
      ],
      "js": [
        "jquery-1.11.1.js",
        "contentScript.js"
      ]
    },
    {
      "matches": [
        "*://www.google.co.uk/*",
        "*://www.google.com/*"
      ],
      "js": [
        "jquery-1.11.1.js",
        "queryTime.js"
      ]
    }
  ]
}