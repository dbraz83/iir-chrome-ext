{
  "name": "IIR Chrome Extension",
  "version": "0.2",
  "manifest_version": 2,
  "description": "Not just mucking about now, this thing is starting to resembling a chrome extension",
  "background": {
    "scripts": [
      "jquery-1.11.1.js",
      "background.js"
    ]
  },
  "permissions": [
    "storage",
    "bookmarks",
    "tabs",
    "cookies",
    "unlimitedStorage"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://www.google.co.uk/search*",
        "*://www.google.com/search*"
      ],
      "js": [
        "jquery-1.11.1.js",
        "contentScript.js"
      ]
    }
  ]
}